<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cryptocurrency.CompareFragment">

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/compareFragment_bitcoin"
        android:layout_width="205dp"
        android:layout_height="400dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/compareFragment_ethereum"
        android:layout_width="205dp"
        android:layout_height="400dp"
        app:layout_constraintStart_toEndOf="@+id/compareFragment_bitcoin"
        app:layout_constraintTop_toTopOf="parent" />

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/compareFragment_compare"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/compareFragment_ethereum" />


</androidx.constraintlayout.widget.ConstraintLayout>